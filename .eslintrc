{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "defaultParams": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "globals": {},
  "rules": {
    "semi": [0],
    "max-len": [0],
    "react/forbid-prop-types": [0],
    "arrow-parens": [0],
    "no-param-reassign": [0],
    "arrow-body-style": [0],
    "react/no-danger": [0],
    "react/jsx-filename-extension": [0],
    "import/no-named-as-default": [0],
    "class-methods-use-this": [0],
    "no-return-assign": [0],
    "no-underscore-dangle": [0],
    "no-undef": [0],
    "object-curly-newline": [0],
    "no-plusplus": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "jsx-a11y/click-events-have-key-events": [0],
    "jsx-a11y/no-static-element-interactions": [0],
  }
}
